<h1>Listing#new</h1>

<%= simple_form_for @listing do |f| %>
<%= f.input :vehicle_type %>
<%= f.input :name %>
<%= f.input :street %>
<%= f.input :zip%>
<%= f.input :city%>
<%= f.input :country%>
<%= f.input :start_date, as: :date, html5: true %>
<%= f.input :end_date, as: :date, html5: true %>
<%= f.submit, class: "btn-primary" %>
<% end %>

